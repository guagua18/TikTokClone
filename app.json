{
  "expo": {
    "name": "TikTokClone",
    "slug": "TikTokClone",
    "version": "1.0.0",
    "scheme": "not-just-tik-tok",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.18laq99.TikTokClone",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      [
        "expo-camera",
        {
          "cameraPermission": "允许访问您的相机以拍照和录像。",
          "microphonePermission": "允许访问您的麦克风以录制视频中的声音。"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "246d0933-007c-4df6-b0ed-972785e48c25"
      }
    }
  }
}
